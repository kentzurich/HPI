<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="description">
    <meta content="" name="keywords">
    
    @await Component.InvokeAsync("Icon")

    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.4/font/bootstrap-icons.css">
    <link href="/lib/customer/fontawesome-free/css/all.min.css" rel="stylesheet">
    <link href="/lib/customer/animate.css/animate.min.css" rel="stylesheet">
    <link href="/lib/customer/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="/lib/customer/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="/lib/customer/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="/lib/customer/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="/lib/customer/remixicon/remixicon.css" rel="stylesheet">
    <link href="/lib/customer/swiper/swiper-bundle.min.css" rel="stylesheet">

    <link href="~/css/customer.css" rel="stylesheet" asp-append-version="true">
</head>
<body>
    
    <partial name="Customer/_HeaderLayout" />

    <main role="main" id="main">
        <partial name="_Notification" />
        @RenderBody()
    </main>

    @await Component.InvokeAsync("Footer")

    <script src="/lib/customer/purecounter/purecounter_vanilla.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.min.js" integrity="sha384-Y4oOpwW3duJdCWv5ly8SCFYWqFDsfob/3GkgExXKV4idmbt98QcxXYs9UoXAB7BZ" crossorigin="anonymous"></script>
    <script src="/lib/customer/glightbox/js/glightbox.min.js"></script>
    <script src="/lib/customer/swiper/swiper-bundle.min.js"></script>
    <script src="~/js/customer.js" asp-append-version="true"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
